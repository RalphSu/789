            }

            oIframe.style.height = h + "px";
            if(window.top!=null && window != window.top){
                var mainFrame = window.top.document.getElementById("iframe");
                if(mainFrame){
                    window.top.iframeFitHeight(mainFrame);
                }
            }
        } catch(e){}
